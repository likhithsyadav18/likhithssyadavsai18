def printPascal(rows):  #C(i, j) = C(i, j-1) * (i - j + 1) / j
    for line in range(1, rows + 1):
        coeff = 1 # Binomial coeff of C(i, j)
        for col in range(1, line + 1):
            print(coeff, end = " ",sep=" ")
            coeff = int(coeff * (line - col) / col)
        print('\n')

printPascal(10)
