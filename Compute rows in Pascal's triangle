def printPascal(rows): 
    arr=[]
    for line in range(rows):
        arr.append([])
        arr[line].append(1)
        for col in range(1,line):
            arr[line].append(arr[line-1][col-1]+arr[line-1][col])
        if(rows!=0):
            arr[line].append(1)

    for i in range(rows):
        for j in range(0,i+1):
            print(arr[i][j],end=" ",sep=" ")
        print()

printPascal(18)
